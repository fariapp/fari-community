---
import { CreatorsProjectSorter } from "src/domains/creators/CreatorsProjectsSorter";
import { loader } from "../catalog/loader";
import Layout from "../layouts/Layout.astro";

const allCreators = await loader.getAllCreators();
const creatorsAndProjects = CreatorsProjectSorter.sort(allCreators);

const top12Creators = creatorsAndProjects.slice(0, 12);

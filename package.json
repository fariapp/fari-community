{
  "name": "fari-community",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "check": "astro check",
    "preview": "astro preview",
    "astro": "astro",
    "live": "bun run build && bun deploy:preview",
    "beta": "bun run build && bun deploy:preview -- --alias beta",
    "alpha": "bun run build && bun deploy:preview -- --alias alpha",
    "prod": "bun run build && bun deploy:preview -- --prod",
    "deploy:preview": "netlify deploy --site b106e8c1-1ca6-4da3-84d7-d06d7fe1ec68",
    "deploy:prod": "netlify deploy --site b106e8c1-1ca6-4da3-84d7-d06d7fe1ec68 --prod",
    "prepare": "husky install"
  },
  "dependencies": {
    "@astrojs/check": "^0.7.0",
    "@astrojs/react": "^3.5.0",
    "@astrojs/tailwind": "^5.1.0",
    "@tailwindcss/typography": "^0.5.13",
    "astro": "^4.10.1",
    "astro-relative-links": "^0.3.9",
    "lodash": "^4.17.21",
    "netlify-cli": "^17.26.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "remarkable": "^2.0.1",
    "tailwindcss": "^3.4.4"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/lodash": "^4.17.5",
    "@types/remarkable": "^2.0.8",
    "typescript": "^5.4.5",
    "@types/node": "^20.14.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.5"
  }
}
